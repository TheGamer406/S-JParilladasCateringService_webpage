{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "ignoreDeprecations": "6.0",
    "jsx": "preserve",
    "jsxImportSource": "astro/jsx-runtime",
    "types": ["astro/astro-jsx", "vite/client"],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}